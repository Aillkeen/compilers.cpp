// nested IF-ELSE statements test for ASSEMBLY CODE GENERATION

int nestedifElse() {
	
	
	if(1 < 3){
		int y = 0;
		
		if (1 < 2) {
			y = 2;
		}
		
	} else {
	
		int y = 0;
		int z = 1;
		if ( y == 1) {
			int x = 0;
		
		
		} else  {
			int x = 2;
			if (1 < 3)  {
				y = 0;
			}
			
		}
		
	}
	
	return 0;
}